{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh3/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "leading_diamond": "",
          "style": "diamond",
          "trailing_diamond": "",
          "type": "os"
        },
        {
          "background": "#0000ff",
          "foreground": "#000000",
          "powerline_symbol": "",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "type": "root"
        },
        {
          "background": "#0000ff",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "properties": {
            "postfix": " ",
            "style": "full"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#D4E157",
          "foreground": "#000000",
          "powerline_symbol": "",
          "properties": {
            "prefix": " ",
            "style": "full"
          },
          "style": "powerline",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#D4E157",
          "foreground": "#000000",
          "leading_diamond": "",
          "properties": {
            "postfix": " <#000000></>"
          },
          "style": "diamond",
          "type": "python"
        },
        {
          "background": "#7FD5EA",
          "display_version": true,
          "foreground": "#ffffff",
          "leading_diamond": "",
          "properties": {
            "postfix": " <#000000></>"
          },
          "style": "diamond",
          "type": "go"
        },
        {
          "background": "#ffffff",
          "foreground": "#000000",
          "leading_diamond": "",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "time"
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#000000",
          "background": "#00ffff",
          "properties": {
            "prefix": " \uE77F "
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#D4E157",
          "properties": {
            "always_enabled": true,
            "display_exit_code": false,
            "error_color": "#FF5252",
            "prefix": "❯"
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ]
}
